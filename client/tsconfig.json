{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "composite": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "~/*": [ "./src/*" ],
      "~store/*": [ "./src/store/*" ],
      "~components/*": [ "./src/components/*" ]
    }
  },
  "references": [
    {
      "path": "../server"
    }
  ]
}
